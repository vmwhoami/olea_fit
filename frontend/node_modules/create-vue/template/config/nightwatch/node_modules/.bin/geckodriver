#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/haoqun/Developer/vuejs/create-vue/node_modules/.pnpm/geckodriver@4.4.1/node_modules/geckodriver/bin/node_modules:/Users/haoqun/Developer/vuejs/create-vue/node_modules/.pnpm/geckodriver@4.4.1/node_modules/geckodriver/node_modules:/Users/haoqun/Developer/vuejs/create-vue/node_modules/.pnpm/geckodriver@4.4.1/node_modules:/Users/haoqun/Developer/vuejs/create-vue/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/haoqun/Developer/vuejs/create-vue/node_modules/.pnpm/geckodriver@4.4.1/node_modules/geckodriver/bin/node_modules:/Users/haoqun/Developer/vuejs/create-vue/node_modules/.pnpm/geckodriver@4.4.1/node_modules/geckodriver/node_modules:/Users/haoqun/Developer/vuejs/create-vue/node_modules/.pnpm/geckodriver@4.4.1/node_modules:/Users/haoqun/Developer/vuejs/create-vue/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../geckodriver/bin/geckodriver.js" "$@"
else
  exec node  "$basedir/../geckodriver/bin/geckodriver.js" "$@"
fi
